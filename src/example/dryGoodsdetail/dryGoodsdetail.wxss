/*进度条*/
.progress {
    position: relative;
    width: 500rpx; }
.progress .progress-bg {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-top: -2px;
    margin-left: -250rpx;
    width: 500rpx;
    height: 4px;
    background: #ebebeb;
    border-radius: 4px; }
.progress .progress-bar {
    position: absolute;
    left: -2rpx;
    top:0;
    /*top: 50%;*/
    /*margin-top: -2px;*/
    width: 0;
    height: 4px;
    background: #3088ff;
    border-radius: 4px; }
.progress .progress-bar .control {
    position: absolute;
    right: -8px;
    top: -6px;
    float: right;
    width: 16px;
    height: 16px;
    background: #eee;
    border-radius: 50%; }

