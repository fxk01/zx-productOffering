@import "../detail/detail.wxss";
@import "../template/jMheader/jMheader.wxss";
@import "../wxParse/wxParse.wxss";
.content {
    padding:0 25rpx 66rpx;
}

.deil_title{
    font-weight:600;
    padding: 60rpx 12rpx 30rpx;
    line-height:48rpx;
}

.deil_timer{
    padding: 0 12rpx;
}
.deil_content .con_txt{
    color: #161616;
    font-size:24rpx;
    line-height:40rpx;
}

.deil_content .txt{
    text-indent:48rpx;
    color: #161616;
    line-height:44rpx;
    font-size: 30rpx;
}

.overGame{
    border:2rpx solid #838383;
    border-radius:14rpx;
    color:#fff;
    width:90%;
    font-size:32rpx;
    height:106rpx;
    line-height:106rpx;
    background-color:#a5a5a5;
    text-align:center;
    margin:0 auto 60rpx;
}

.beginGame{
    height:90rpx;
    margin-bottom: 74rpx;
    /*color: #040404;*/
}


.questionBtn1{
    position:fixed;
    z-index:999;
    left:0;
    bottom:0;
    color:#fff;
    font-size:28rpx;
    text-align:center;
    width:100%;
    padding: 0 30rpx 18rpx;
    background-color: #fff;
}

.Btn_txt{
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    background-color:#0078ff;
    border-radius:10rpx;
}

button:after{
    border:0px solid rgba(255, 255,255, 0);
    width:0;
    height: 0;
}

.design_con {
    padding:0 36rpx;
}

.design_conTitBox .design_conTit{
    display: inline-block;
    margin-bottom: 24rpx;
    color: #5993f5;
    font-weight: 500;
    font-size: 40rpx;
    letter-spacing: 2rpx;
}

.design_mess{
    margin-bottom: 30rpx;
    font-size: 28rpx;
    color: #565656;
}
