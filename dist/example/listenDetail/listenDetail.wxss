/*进度条*/
.progress {
    position: relative;
    width: 500rpx; }
.progress .progress-bg {
    position: absolute;
    left: 50%;
    top: 50%;
    margin-top: -2px;
    margin-left: -250rpx;
    width: 500rpx;
    height: 4px;
    background: #ebebeb;
    border-radius: 4px; }
.progress .progress-bar {
    position: absolute;
    left: -2rpx;
    top:0;
    /*top: 50%;*/
    /*margin-top: -2px;*/
    width: 0;
    height: 4px;
    background: #3088ff;
    border-radius: 4px; }

.progress .progress-bar .control {
    position: absolute;
    right: -3px;
    top: -4px;
    float: right;
    width: 6px;
    height: 12px;
    background: #3088ff;
    border-radius: 10rpx;
}

.control:before{
    content: "";
    width: 12px;
    height: 12px;
    background: transparent;
    position: absolute;
    left: -12px;
    top: 0;
}

.control:after{
    content: "";
    width: 12px;
    height: 12px;
    background: transparent;
    position: absolute;
    right: -12px;
    top: 0;
}

