                </view>
            </view>

            <view class="flex-1 headCon">
                <view class="headconChoice headconPosi2">
                    <image src="../images/jm_opeicon2.png" mode="aspectFill"></image>
                    <view class="headconTxt">贴心解决方案</view>
                </view>
            </view>

            <view class="flex-1 headCon">
