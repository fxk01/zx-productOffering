@import "../template/jMheader/jMheader.wxss";
.container{
    background-color: #fff;
    line-height: normal;
}

.jm_header .jm_headTxt {font-weight:500;
    /*color:#323232;*/
    font-size:30rpx;
}

.torialLine {
    width:83%;
    margin:0 auto;
    height:2rpx;
    background-color:#f0f0f0;
}

.design_con{
    padding:0 36rpx;
}

.con_top{
    margin-top: 22rpx;
    line-height: 42rpx;
    font-size: 24rpx;
    color: #878787;
}

.design_conTitBox{
    padding-top: 36rpx;
}

.design_conTitBox .design_conTit{
    display: inline-block;
    padding-left: 20rpx;
    margin-bottom: 24rpx;
    color: #5993f5;
    font-weight: 500;
    font-size: 40rpx;
    letter-spacing: 2rpx;
    border-left: 4rpx solid #ff0826;
}

.con_achieveBox{
    margin-bottom: 26rpx;
}

.con_achieveBox .achieveBox:nth-child(1){
    margin-right:16rpx ;
}

.con_achieveBox .achieveBox{
    border:2rpx solid #eee;
    padding:36rpx 64rpx 4rpx ;
    text-align: center;
}

.achieveBox image{
    width:64rpx;
    height: 64rpx;
}

.achieveBox .design_conSub{
    padding-top: 22rpx;
    padding-bottom: 10rpx;
    font-size: 30rpx;
    color: #565656;
}

.achieveBox .design_conDetail{
    min-height: 70rpx;
    font-size: 24rpx;
    line-height: 32rpx;
    color: #878787;
}

.con_circleBox{
    padding: 18rpx 0;
    margin-bottom: 22rpx;
    border:2rpx solid #eee;
}

.circleBox{
    text-align: center;
}

.circleBox .circle_Tit{
    font-weight: 600;
    width: 120rpx;
    margin: 0 auto 12rpx;
    font-size:36rpx;
    color: #5993f5;
    padding-bottom: 20rpx;
    border-bottom:4rpx solid #5993f5;
}

.circleBox .circle_Sub{
    line-height: 48rpx;
    font-size:24rpx;
    color: #606060;
}

.circle_Sub .circle_Txt{

}

.design_mess{
     margin-bottom: 80rpx;
     font-size: 28rpx;
     color: #565656;
 }

.questionBtn1{
    color:#fff;
    font-size:28rpx;
    position: fixed;
    z-index: 999;
    left:0;
    bottom: 0;
    text-align:center;
    width:100%;
    padding: 0 20rpx 18rpx;
    background-color: #fff;
}

.Btn_txt{
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    background-color:#0078ff;
    border-radius:10rpx;
}

button:after{
    border:0px solid rgba(255, 255,255, 0);
    width:0;
    height: 0;
}



