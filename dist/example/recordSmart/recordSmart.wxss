.recordcon_new{
    position: relative;
    padding: 344rpx 16rpx 10rpx;
    background-color: #eae8e8;
}

.recordcon_Box{
    background-color: #fff;
    margin-bottom: 10rpx;
    padding-left:34rpx;
}

.recordcon_Box:nth-child(1){
    width:95.73%;
   margin: 0 auto;
    position: absolute;
    top:-36rpx;
}

.recordcon_Tit{
    border-bottom: 2rpx solid #e5e5e5;
    margin-right: 22rpx;
    letter-spacing: 2rpx;
    padding: 36rpx 0 18rpx;
    font-weight: 600;
    font-size: 38rpx;
    color: #707070;
}

.recordcon_content{
    padding:18rpx 34rpx 16rpx 0;
    overflow: hidden;
}

.recordcon_contentL{
    width:318rpx;
    float: left;
}

.recordcon_contentR{
    float: left;
}

.recordcon_content .recordcon_detail{
    line-height: 46rpx;
    color: #acacac;
}

.recordcon_content .recordcon_detail image{
    width: 20rpx;
    height: 20rpx;
    margin-right: 18rpx;
}

.recordcon_content .recordcon_detail text{
    display: inline-block;
    font-size: 24rpx;
}

.recordcon_foot{
    padding:0 34rpx 40rpx 0;
}

.recordcon_foot text{
    display: inline-block;
    line-height: 30rpx;
    height: 30rpx;
    margin-right: 10rpx;
    font-size: 18rpx;
    text-align: center;
    border:2rpx solid #dadada;
    border-radius: 4rpx;
}

.recordcon_foot text:nth-child(odd) {
    min-width: 98rpx;
    color:#3569ff;
}
.recordcon_foot text:nth-child(even) {
    min-width: 78rpx;
    color:#ff8b35;
}








